@model IEnumerable<EXSM3935_C_Sharp_Final_Project.Models.Accounttype>

@{
    ViewData["Title"] = "Account Types";
}

<h1>Account Types</h1>

<p>
    <a asp-action="Create">Create New Type</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                Account Type ID
            </th>
            <th>
                <!--@Html.DisplayNameFor(model => model.Name)-->
                Account Type Name
            </th>
            <th>
                <!--@Html.DisplayNameFor(model => model.InterestRate)-->
                Interest Rate
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.StrId)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Name)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.InterestRate)%
                </td>
                <td>
                    <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                    <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                    <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>
